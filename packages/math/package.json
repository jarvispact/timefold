{
  "name": "@timefold/math",
  "version": "0.0.3",
  "description": "matrix, vector and math implementation for timefold",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/math.umd.js",
  "module": "./dist/math.js",
  "types": "./dist/math.d.ts",
  "publishConfig": {
    "access": "public"
