{
  "name": "timefold",
  "version": "0.0.1",
  "description": "A blazingly fast, ecs powered game engine for the web",
  "type": "module",
  "workspaces": [
    "packages/ecs",
    "packages/math",
    "packages/webgpu",
    "packages/obj",
    "packages/engine",
    "packages/examples"
  ],
  "scripts": {
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "test": "npm run test:run --workspaces --if-present",
    "build": "npm run build --workspaces --if-present",
    "publish-to-npm": "npm run build --workspaces --if-present && npm run publish-to-npm --workspaces --if-present"
  },
  "keywords": [
    "game",
    "engine",
    "ecs",
    "webgpu"
  ],
  "author": "jarvispact <jarvispact@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/node": "^22.10.5",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.14.0",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.19.1",
    "vite": "^6.0.7",
    "vitest": "^2.1.8",
    "@webgpu/types": "^0.1.52"
  }
}
